<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sxl.entity.Strategy" table="t_strategy">
        <id name="sid" column="sid">
            <generator class="native"/>
        </id>
        <property name="name" column="name"/>
        <property name="title" column="strategy_title"/>
        <property name="inner" column="strategy_inner"/>
        <property name="path" column="img_path"/>
        <property name="uploadTime" column="upload_time"/>
    </class>
</hibernate-mapping>